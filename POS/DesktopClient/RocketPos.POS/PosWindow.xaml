<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
	xmlns:ic="http://schemas.microsoft.com/expression/2010/interactions"
	x:Class="POS.PosWindow"
	x:Name="Window"
	xmlns:local="clr-namespace:RocketPos.POS" 
    xmlns:views="clr-namespace:POS.ViewModels.PosMainView.Views"
    VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
        WindowState="Maximized"
        Title="Point-of-Sale Window"
        DataContext="{Binding PosMainView, Source={StaticResource Locator}}">
    <Grid>
        <views:PosMainView/>
    </Grid>

</Window>