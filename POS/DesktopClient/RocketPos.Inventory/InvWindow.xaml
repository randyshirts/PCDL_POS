<Window x:Class="Inventory.InvWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:RocketPos.Inventory"
        xmlns:views="clr-namespace:Inventory.ViewModels.MainView.Views"
        VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
        WindowState="Maximized"
        Title="Inventory Window"
        DataContext="{Binding MainView, Source={StaticResource Locator}}">
    <Grid>
        <views:MainView/>
    </Grid>
</Window>
